{
  "version": "1.0.0",
  "id": "@shopgate-project/swell",
  "components": [
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "swellCustomer",
      "path": "frontend/reducers/swellCustomer.js",
      "type": "reducers"
    },
    {
      "type": "portals",
      "target": "app.globals",
      "path": "frontend/portals/GlobalCssRules",
      "id": "GlobalCssRules"
    },
    {
      "id": "SwellWelcome",
      "path": "frontend/widgets/SwellWelcome/index.jsx",
      "type": "widgets",
      "description": "Swell welcome widget"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    }
  ],
  "configuration": {
    "swellSdkUrl": {
      "type": "admin",
      "destination": "frontend",
      "default": "https://cdn.swellrewards.com/loader/JiY-wIwEaNemLIT7R1Nbhg.js",
      "params": {
        "type": "text",
        "label": "SRC to Swell JavaScript sdk"
      }
    }
  }
}
